        "projects/ngx-textarea/src/public-api"
      ],
      "@b2b/ngx-password": [
        "projects/ngx-password/src/public-api"
      ],
      "@b2b/ngx-file": [
        "projects/ngx-file/src/public-api"
      ],
      "@b2b/ngx-tel": [
        "projects/ngx-tel/src/public-api"
      ],
      "@b2b/ngx-radio": [
        "projects/ngx-radio/src/public-api"
      ],
      "@b2b/ngx-images-carousel": [
        "projects/ngx-images-carousel/src/public-api"
