{
	"name": "fe-b2b",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"dev:ssr": "node --max-old-space-size=4096 ./node_modules/.bin/ng run fe-b2b:serve-ssr",
		"serve:ssr": "node dist/fe-b2b/server/main.js",
		"build:ssr": "ng build && ng run fe-b2b:server",
		"prerender": "ng run fe-b2b:prerender",
		"format": "prettier --write \"src\"",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"npm run format",
			"git add"
		]
	},
	"private": true,
	"dependencies": {
