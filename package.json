{
	"name": "fe-b2b",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"serve:admin": "ng serve admin",
		"build:admin": "ng build --project='admin' --configuration production",
		"dev:ssr": "node --max-old-space-size=4096 ./node_modules/.bin/ng run fe-b2b:serve-ssr",
		"serve:ssr": "node dist/fe-b2b/server/main.js",
		"build:ssr": "ng build --configuration production && ng run fe-b2b:server",
		"build:serve:ssr": "npm run build:ssr && npm run serve:ssr",
		"prerender": "ng run fe-b2b:prerender",
		"format": "prettier --write src admin",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"npm run format",
			"git add"
		]
	},
	"private": true,
